{
    "name": "LinkedIn Scraper",
    "description": "This is used to scrape a LinkedIn profile",
    "version": "1",
    "manifest_version": 2,
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "notifications",
        "tabs",
        "history",
        "activeTab",
        "storage",
        "http://www.linkedin.com/in/*"
    ],
    "browser_action": {
        "default_icon": "linkedin_icon.png"
    }
}