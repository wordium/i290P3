{
  "manifest_version": 2,

  "name": "LinkedIn Scraper",
  "description": "This is a Linked In Scraper",
  "version": "1.0",
  "background": {
    //"page" : "background.html"
    "scripts": ["background.js"]
  },
  "permissions": [
    "history",
    "tabs",
    "activeTab",
    "http://www.linkedin.com/in/*"
  ],
  "browser_action": {
    "default_icon": "linkedin_icon.png"
    //"default_popup": "background.html"
  },
  /*"content_scripts" : [{
    "js": ["jquery.2.0.3.js", "linkedin-scrape.js"],
    "matches": ["http://www.linkedin.com/in/*", "http://www.linkedin.com/in/*"],
    "css": ["yelp-tagger.css"]
  }],*/
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://api.del.icio.us; object-src 'self'"
}